<configuration>
	<appSettings>
		<!-- Nível mínimo de log -->
		<add key="CustomLogger:MinimumLogLevel" value="Information" />
		<add key="CustomLogger:ServiceName" value="PaymentAPI" />
		<add key="CustomLogger:Environment" value="Development" />

		<!-- Buffer (armazenamento) -->
		<add key="CustomLogger:Buffer:Enabled" value="true" />
		<add key="CustomLogger:Buffer:MaxSize" value="300" />

		<!-- Batch (agrupamento/envio) -->
		<add key="CustomLogger:Batch:BatchSize" value="30" />
		<add key="CustomLogger:Batch:FlushIntervalMs" value="5000" />

		<!-- Console sink -->
		<add key="CustomLogger:Sinks:Console:Enabled" value="true" />

		<!-- File sink -->
		<add key="CustomLogger:Sinks:File:Enabled" value="true" />
		<add key="CustomLogger:Sinks:File:Path" value="C:/logs/PaymentAPI/logs-18-02.log" />

		<!-- BlobStorage sink (opcional) -->
		<add key="CustomLogger:Sinks:BlobStorage:Enabled" value="false" />
		<add key="CustomLogger:Sinks:BlobStorage:ConnectionString" value="DefaultEndpointsProtocol=https;..." />
		<add key="CustomLogger:Sinks:BlobStorage:ContainerName" value="logs" />

		<!-- Dynatrace sink (opcional) -->
		<add key="CustomLogger:Sinks:Dynatrace:Enabled" value="false" />
		<add key="CustomLogger:Sinks:Dynatrace:Endpoint" value="" />
		<add key="CustomLogger:Sinks:Dynatrace:ApiToken" value="" />
		<add key="CustomLogger:Sinks:Dynatrace:TimeoutSeconds" value="3" />

		<!-- OpenTelemetry Settings -->
		<add key="CustomLogger:OpenTelemetry:Enabled" value="true" />
		<add key="CustomLogger:OpenTelemetry:Exporter" value="Otlp" />
		<add key="CustomLogger:OpenTelemetry:Instrumentations:AspNetCore" value="false" />
		<add key="CustomLogger:OpenTelemetry:Instrumentations:HttpClient" value="true" />
		<add key="CustomLogger:OpenTelemetry:Instrumentations:Oracle" value="false" />

		<add key="webpages:Version" value="3.0.0.0" />
		<add key="webpages:Enabled" value="false" />
		<add key="ClientValidationEnabled" value="true" />
		<add key="UnobtrusiveJavaScriptEnabled" value="true" />
	</appSettings>
